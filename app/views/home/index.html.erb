<section class="box-team">
  <header>
    <h1>Our Team(Zombies)</h1>
  </header>
  <div id="list-team" class="clearfix">
    <% @zombies.each do |zombie| %>
      <article class="zombie-profile lfloat">
          <p><%= zombie[:name] %></p>
          <div class="image">
            <%= link_to(zombie_path(zombie)) do %>
              <%= image_tag(zombie[:image]) %>
            <% end %>
          </div>
      </article>
    <% end %>
  </div>
</section>